{
  "multipart": [
    {
      "apply": {
        "model": "ltextras:block/thorn_stem"
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "uvlock": true
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "x": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "x": 90,
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "x": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "x": 90,
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "x": 90,
        "y": 270,
        "uvlock": true
      }
    }
  ]
}