{
  "multipart": [
    {
      "apply": {
        "model": "ltextras:block/thorn_stem"
      }
    },
    {
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "uvlock": true
      },
      "when": {
        "down": "true"
      }
    },
    {
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "uvlock": true,
        "x": 180
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "uvlock": true,
        "x": 90,
        "y": 180
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "uvlock": true,
        "x": 90,
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "ltextras:block/thorn_stem_connection",
        "uvlock": true,
        "x": 90,
        "y": 270
      },
      "when": {
        "east": "true"
      }
    }
  ]
}